{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "guard-for-in": 0,
        "no-shadow": 0,
        "no-restricted-syntax": 0,
        "class-methods-use-this": 0,
        "no-plusplus": 0
    }
}
